#!/command/execlineb -P
fdmove -c 2 1

s6-envdir /etc/koha-envvars

/usr/bin/starman -M FindBin --socket /var/run/koha/library/plack.sock --max-requests 50 --workers 2 -E deployment --user=library-koha --group=library-koha --access-log /var/log/koha/library/plack.log --error-log /var/log/koha/library/plack-error.log "/etc/koha/plack.psgi" 
